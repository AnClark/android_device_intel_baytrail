    <Sensor>
      <SensorID>8</SensorID>
      <SensorName>Modem_BB</SensorName>
      <SensorPath>none</SensorPath>
      <InputTemp>invalid</InputTemp>
      <HighTemp>invalid</HighTemp>
      <LowTemp>invalid</LowTemp>
    </Sensor>
  </Zone>
 </thermalconfig>
