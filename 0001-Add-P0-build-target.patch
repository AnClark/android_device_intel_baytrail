+else
+LOCAL_REQUIRED_MODULES += ParameterFrameworkConfigurationVibratorNoTuning.xml
+endif
+
+include $(BUILD_PHONY_PACKAGE)
+
+##################################################
+# MODULES REQUIRED by parameter-framework.vibrator.baytrail Package
