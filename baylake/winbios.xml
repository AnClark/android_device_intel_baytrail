<?xml version="1.0" encoding="utf-8"?>
<flashfile version="1">
    <id>fastboot</id>
    <platform>baylake</platform>
        <code_group name="MOS_KERNEL">
            <file TYPE="MOS_KERNEL">
                <name>mos_kernel.efi</name>
                <version>eng.lab</version>
            </file></code_group>
        <code_group name="MOS_RAMDISK">
            <file TYPE="MOS_RAMDISK">
                <name>mos_ramdisk.img</name>
                <version>eng.lab</version>
            </file></code_group>
        <code_group name="SYSTEM">
            <file TYPE="SYSTEM">
                <name>system.img.gz</name>
                <version>eng.lab</version>
            </file></code_group>
        <command>
            <string>fastboot oem disable_kboot</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Switch to flash mode</description>
        </command>
        <command>
            <string>fastboot flash /mnt/mos_kernel.efi $mos_kernel_file</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Flash kernel</description>
        </command>
        <command>
            <string>fastboot flash /mnt/mos_ramdisk.img $mos_ramdisk_file</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Flash ramdisk</description>
        </command>
        <command>
            <string>fastboot erase cache</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Erasing cache</description>
        </command>
        <command>
            <string>fastboot erase system</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Erasing system</description>
        </command>
        <command>
            <string>fastboot erase data</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Erasing data</description>
        </command>
        <command>
            <string>fastboot flash system $system_file</string>
            <timeout>300000</timeout>
            <retry>2</retry>
            <description>Flashing system</description>
        </command>
        <command>
            <string>fastboot continue</string>
            <timeout>60000</timeout>
            <retry>2</retry>
            <description>Reboot system</description>
        </command>
</flashfile>
