            <Configuration Name="InCall.Earpiece">
                <CompoundRule Type="All">
                    <CompoundRule Type="All">
                        <SelectionCriterionRule SelectionCriterion="SelectedOutputDevice" MatchesWhen="Includes" Value="Earpiece"/>
                    </CompoundRule>
                    <CompoundRule Type="Any">
                        <SelectionCriterionRule SelectionCriterion="Mode" MatchesWhen="Is" Value="InVoipCall"/>
                    </CompoundRule>
                </CompoundRule>
            </Configuration>
        </Configurations>
        <ConfigurableElements>
