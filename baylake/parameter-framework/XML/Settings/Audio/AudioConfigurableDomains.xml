            </Configuration>
            <Configuration Name="Multimedia.Earpiece">
                <ConfigurableElement Path="/Audio/REALTEK/SOUND_CARD/OUTPUT/DIGITAL/VOLUME/DAC1">
                    <IntegerParameter Name="DAC1">170 170</IntegerParameter>
                </ConfigurableElement>
            </Configuration>
            <Configuration Name="InCall.IHF">
                <ConfigurableElement Path="/Audio/REALTEK/SOUND_CARD/OUTPUT/SPEAKER/ANALOG/VOLUME">
                    <IntegerParameter Name="VOLUME">31 31</IntegerParameter>
                </ConfigurableElement>
                <ConfigurableElement Path="/Audio/REALTEK/SOUND_CARD/OUTPUT/DIGITAL/VOLUME/DAC1">
                    <IntegerParameter Name="DAC1">120 120</IntegerParameter>
                </ConfigurableElement>
            </Configuration>
            <Configuration Name="InCall.Headset">
                <ConfigurableElement Path="/Audio/REALTEK/SOUND_CARD/OUTPUT/HEADPHONE/ANALOG/VOLUME">
                    <IntegerParameter Name="VOLUME">31 31</IntegerParameter>
                </ConfigurableElement>
                <ConfigurableElement Path="/Audio/REALTEK/SOUND_CARD/OUTPUT/DIGITAL/VOLUME/DAC1">
                    <IntegerParameter Name="DAC1">120 120</IntegerParameter>
                </ConfigurableElement>
            </Configuration>
            <Configuration Name="InCall.Earpiece">
                <ConfigurableElement Path="/Audio/REALTEK/SOUND_CARD/OUTPUT/DIGITAL/VOLUME/DAC1">
                    <IntegerParameter Name="DAC1">100 100</IntegerParameter>
                </ConfigurableElement>
            </Configuration>
        </Settings>
    </ConfigurableDomain>


  <ConfigurableDomain Name="AudioConfiguration" SequenceAware="false">
        <Configurations>
            <Configuration Name="Default">
                <CompoundRule Type="All"/>
            </Configuration>
        </Configurations>
        <ConfigurableElements>
            <ConfigurableElement Path="/Audio/CONFIGURATION/ALSA_CONF"/>
            <ConfigurableElement Path="/Audio/CONFIGURATION/IFX_MODEM"/>
            <ConfigurableElement Path="/Audio/CONFIGURATION/FM_CONF"/>
        </ConfigurableElements>
        <Settings>
            <Configuration Name="Default">
                <ConfigurableElement Path="/Audio/CONFIGURATION/ALSA_CONF">
                    <Component Name="ALSA_CONF">
                        <Component Name="IN">
                            <IntegerParameter Name="DEFAULT_SAMPLE_RATE">48000</IntegerParameter>
                        </Component>
                        <Component Name="OUT">
                            <IntegerParameter Name="DEFAULT_SAMPLE_RATE">48000</IntegerParameter>
                        </Component>
                    </Component>
                </ConfigurableElement>
                <ConfigurableElement Path="/Audio/CONFIGURATION/IFX_MODEM">
                    <Component Name="IFX_MODEM">
                        <Component Name="I2S1">
                            <EnumParameter Name="CLK_SELECT">IFX_CLK0</EnumParameter>
                        </Component>
                        <Component Name="I2S2">
                            <EnumParameter Name="CLK_SELECT">IFX_CLK0</EnumParameter>
                        </Component>
                    </Component>
                </ConfigurableElement>
                <ConfigurableElement Path="/Audio/CONFIGURATION/FM_CONF">
                    <Component Name="FM_CONF">
                        <Component Name="SPEAKER">
                            <IntegerParameter Name="VOLUME">55</IntegerParameter>
                        </Component>
                        <Component Name="HEADSET">
                            <IntegerParameter Name="VOLUME">62</IntegerParameter>
                        </Component>
                    </Component>
                </ConfigurableElement>
            </Configuration>
        </Settings>
    </ConfigurableDomain>
  <ConfigurableDomain Name="LPE_Mixer" SequenceAware="false">
    <Configurations>
      <Configuration Name="Default">
        <CompoundRule Type="All"/>
      </Configuration>
    </Configurations>
    <ConfigurableElements>
      <ConfigurableElement Path="/Audio/LPE_MIXER/SOUND_CARD/IHF/INPUT_STREAM"/>
      <ConfigurableElement Path="/Audio/LPE_MIXER/SOUND_CARD/HS/INPUT_STREAM"/>
    </ConfigurableElements>
    <Settings>
      <Configuration Name="Default">
        <ConfigurableElement Path="/Audio/LPE_MIXER/SOUND_CARD/IHF/INPUT_STREAM">
          <EnumParameter Name="INPUT_STREAM">MIX</EnumParameter>
        </ConfigurableElement>
        <ConfigurableElement Path="/Audio/LPE_MIXER/SOUND_CARD/HS/INPUT_STREAM">
          <EnumParameter Name="INPUT_STREAM">MIX</EnumParameter>
        </ConfigurableElement>
      </Configuration>
    </Settings>
  </ConfigurableDomain>
</ConfigurableDomains>
